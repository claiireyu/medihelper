{
  "name": "medihelper-backend",
  "version": "1.0.0",
  "description": "Backend API for MediHelper medication management app",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "test:unit": "node --experimental-vm-modules node_modules/.bin/jest --config=jest.unit.config.js",
    "test:integration": "node --experimental-vm-modules node_modules/.bin/jest test/integration",
    "test:integration:coverage": "node --experimental-vm-modules node_modules/.bin/jest test/integration --coverage",
    "test:database": "node --experimental-vm-modules node_modules/.bin/jest test/integration/database.integration.test.js",
    "test:api": "node --experimental-vm-modules node_modules/.bin/jest test/integration/api.integration.test.js"
  },
  "keywords": [
    "medication",
    "health",
    "api",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.9"
  }
}
